{
    "name": "graste/params",
    "type": "library",
    "description": "Array wrapper that eases the retrieval of values. Has parameters, options and settings and traits for inclusion in other libraries.",
    "keywords": ["parameters", "options", "settings", "array", "search", "library"],
    "homepage": "https://github.com/graste/params",
    "license": "MIT",
    "authors": [
        {
            "name": "Steffen Gransow",
            "email": "params@mivesto.de",
            "homepage": "http://mivesto.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "params@mivesto.de",
        "issues": "https://github.com/graste/params/issues",
        "source": "https://github.com/graste/params"
    },
    "prefer-stable": true,
    "require": {
        "php": "^5.6.0 || ^7.0",
        "mtdowling/jmespath.php": "^1.1 || ^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Params\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Params\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
